{
  "name": "typescript-ddd-skeleton",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "url": "https://github.com/CodelyTV/typescript-ddd-skeleton"
  },
  "license": "",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.0"
  },
  "scripts": {
    "dev:cleck:backend": "NODE_ENV=dev ts-node-dev --ignore-watch node_modules  ./src/apps/cleck/backend/start.ts",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run test:unit && npm run test:features",
    "test:unit": "NODE_ENV=test jest",
    "start:cleck:backend": "NODE_ENV=production node dist/src/apps/cleck/backend/start",
    "test:features": "npm run test:cleck:backend:features",
    "test:cleck:backend:features": "NODE_ENV=test cucumber-js -p cleck_backend",
    "build": "npm run build:clean && npm run build:tsc && npm run build:di",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "build:di": "copy 'src/**/*.{json,yaml,html,png}' dist/src",
    "build:clean": "rm -r dist; exit 0"
  },
  "dependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@types/jsonwebtoken": "^9.0.5",
    "awilix": "^10.0.1",
    "axios": "^1.6.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bodybuilder": "^2.5.1",
    "bson": "^6.2.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "convict": "^6.2.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "copy": "^0.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-promise-router": "^4.1.1",
    "express-validator": "^7.0.1",
    "glob": "^10.3.10",
    "helmet": "^7.1.0",
    "http-status": "^1.7.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "nunjucks": "^3.2.4",
    "reflect-metadata": "^0.2.1",
    "request-ip": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "uuid-validate": "^0.0.3",
    "winston": "^3.11.0",
    "wtfnode": "^0.9.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/bson": "^4.0.5",
    "@types/compression": "^1.7.5",
    "@types/connect-flash": "0.0.40",
    "@types/convict": "^6.1.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cookie-session": "^2.0.48",
    "@types/cors": "^2.8.17",
    "@types/cucumber": "^6.0.1",
    "@types/errorhandler": "1.5.3",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/helmet": "0.0.48",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.17",
    "@types/nunjucks": "^3.2.6",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid-validate": "^0.0.3",
    "@types/wtfnode": "^0.7.3",
    "cucumber": "^6.0.5",
    "eslint-config-codely": "^3.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "15.2.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
